#include<iostream>
using namespace std;
int main()
{
    int i,j,n,a[i],h;
    int c=0;
    cout<<"enter the height of A and B "<<endl;
    cin>>h;
    cout<<"enter the number of persons between a and b"<<endl;
    cin>>n;
    cout<<"enter the heights of the persons between a and b";
    for(i=0;i<=n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<=n;i++)
    {
        if(h<a[i])
        {
            c++;
        }
        
        
    }
    cout<<"the minimum no of shot:"<<c;
}
